{
  "name": "node-server-ts",
  "version": "0.0.1",
  "private": "true",
  "description": "Generic boilerplate for Node.js server with TypeScript",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "tsc --build ./tsconfig.json && tsc-alias",
    "prebuild": "rimraf dist",
    "dev": "nodemon",
    "start": "node dist/index.js",
    "lint": "eslint './src/**/*.ts'",
    "test": "CI=true jest",
    "test:local": "jest --verbose",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "boilerplate",
    "node",
    "typescript"
  ],
  "author": "victorlcm",
  "license": "MIT",
  "devDependencies": {
    "@types/config": "0.0.41",
    "@types/jest": "28.1.5",
    "@types/node": "18.0.3",
    "@typescript-eslint/eslint-plugin": "5.30.6",
    "@typescript-eslint/parser": "5.30.6",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "28.1.3",
    "nodemon": "^2.0.19",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "ts-jest": "28.0.5",
    "ts-node": "10.8.2",
    "tsc-alias": "^1.6.11",
    "tsconfig-paths": "^4.0.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "config": "3.3.7",
    "js-yaml": "4.1.0",
    "tslib": "2.4.0"
  }
}
